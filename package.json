{
    "name": "food-truck-challenge",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test --code-coverage --browsers ChromeHeadless --watch=false",
        "test:dev": "ng test --code-coverage",
        "lint": "ng lint food-truck-challenge",
        "lint:fix": "ng lint food-truck-challenge --fix",
        "e2e": "ng e2e",
        "prettier": "pretty-quick --staged",
        "gh-pages": "ng build --prod --base-href /food-truck-challenge/ && npx angular-cli-ghpages"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier",
            "pre-push": "npm run lint && npm run test && npm run gh-pages"
        }
    },
    "dependencies": {
        "@agm/core": "1.1.0",
        "@angular/animations": "9.0.2",
        "@angular/cdk": "8.2.3",
        "@angular/common": "9.0.2",
        "@angular/compiler": "9.0.2",
        "@angular/core": "9.0.2",
        "@angular/flex-layout": "8.0.0-beta.27",
        "@angular/forms": "9.0.2",
        "@angular/material": "8.2.3",
        "@angular/platform-browser": "9.0.2",
        "@angular/platform-browser-dynamic": "9.0.2",
        "@angular/router": "9.0.2",
        "bootstrap": "4.4.1",
        "hammerjs": "2.0.8",
        "rxjs": "6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.3",
        "@angular/cli": "9.0.3",
        "@angular/compiler-cli": "9.0.2",
        "@angular/language-service": "9.0.2",
        "@types/jasmine": "3.5.7",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "^12.11.1",
        "angular-cli-ghpages": "0.6.2",
        "codelyzer": "^5.1.2",
        "husky": "4.2.3",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "4.4.1",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.1",
        "karma-jasmine": "3.1.1",
        "karma-jasmine-html-reporter": "1.5.2",
        "prettier": "1.19.1",
        "pretty-quick": "2.0.1",
        "protractor": "5.4.3",
        "ts-node": "8.6.2",
        "tslint": "5.20.1",
        "tslint-config-airbnb": "5.11.2",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.7.5"
    }
}
