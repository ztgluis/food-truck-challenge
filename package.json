{
    "name": "food-truck-challenge",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test --code-coverage --browsers ChromeHeadless --watch=false",
        "test:dev": "ng test --code-coverage",
        "lint": "ng lint food-truck-challenge",
        "lint:fix": "ng lint food-truck-challenge --fix",
        "e2e": "ng e2e",
        "prettier": "pretty-quick --staged",
        "gh-pages": "ng build --prod --base-href /food-truck-challenge/ && npx angular-cli-ghpages"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier",
            "pre-push": "npm run lint && npm run test && npm run gh-pages"
        }
    },
    "dependencies": {
        "@agm/core": "3.0.0-beta.0",
        "@angular/animations": "10.0.8",
        "@angular/cdk": "10.1.2",
        "@angular/common": "10.0.8",
        "@angular/compiler": "10.0.8",
        "@angular/core": "10.0.8",
        "@angular/flex-layout": "10.0.0-beta.32",
        "@angular/forms": "10.0.8",
        "@angular/material": "10.1.2",
        "@angular/platform-browser": "10.0.8",
        "@angular/platform-browser-dynamic": "10.0.8",
        "@angular/router": "10.0.8",
        "bootstrap": "4.5.2",
        "rxjs": "6.6.2",
        "tslib": "2.0.1",
        "zone.js": "0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1000.5",
        "@angular/cli": "10.0.5",
        "@angular/compiler-cli": "10.0.8",
        "@angular/language-service": "10.0.8",
        "@types/googlemaps": "3.39.11",
        "@types/jasmine": "3.5.12",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "12.12.54",
        "angular-cli-ghpages": "0.6.2",
        "codelyzer": "6.0.0",
        "husky": "4.2.5",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "5.0.2",
        "karma": "5.1.1",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "3.3.0",
        "karma-jasmine-html-reporter": "1.5.0",
        "prettier": "1.19.1",
        "pretty-quick": "2.0.1",
        "protractor": "7.0.0",
        "ts-node": "8.10.2",
        "tslint": "6.1.3",
        "tslint-config-airbnb": "5.11.2",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.9.7"
    }
}
